{
    "manifest_version": 2,
  
    "name": "Google Meet Chat Reader",
    "description": "This extension reads and sends to a locally hosted NodeJS server",
    "version": "1.0",
  
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "https://meet.google.com/",
        "storage"
    ],
    "icons": {
        "128" : "Squawker.png"
    }
  }